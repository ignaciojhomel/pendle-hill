{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "omgwp/testimonials",
  "version": "0.1.0",
  "title": "Testimonials",
  "category": "omgwp",
  "icon": "admin-generic",
  "description": "Testimonials Block tool.",
  "example": {},
  "supports": {
    "html": false,
    "customClassName": true,
    "spacing": {
      "padding": [
        "top",
        "bottom"
      ]
    }
  },
  "textdomain": "testimonials",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "padding": {
            "top": "96px",
            "bottom": "96px"
          }
        }
      }
    },
    "backgroundImage": {
      "type": "object",
      "default": "",
      "attributes": {
        "url": {
          "type": "string",
          "default": ""
        },
        "id": {
          "type": "number",
          "default": 0
        },
        "alt": {
          "type": "string",
          "default": ""
        }
      }
    },
    "sectionTitle": {
      "type": "string",
      "default": ""
    },
    "sectionTitleTag": {
      "type": "string",
      "default": "h2"
    },
    "sectionDescription": {
      "type": "string",
      "default": ""
    },
    "sectionDescriptionTag": {
      "type": "string",
      "default": "p"
    },
    "button": {
      "type": "object",
      "default": "",
      "attributes": {
        "text": {
          "type": "string",
          "default": ""
        },
        "url": {
          "type": "string",
          "default": ""
        }
      }
    },
    "numberOfTestimonials": {
      "type": "number",
      "default": 6
    },
    "blockID": {
      "type": "string",
      "default": ""
    }
  }
}